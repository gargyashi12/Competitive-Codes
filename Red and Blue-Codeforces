#include <bits/stdc++.h>
using namespace std;
int main()
{
    int t;
    cin>>t;
    while(t--)
     { int n;
    cin>>n;
    int a[n],sum1=0,max1=0;
    for(int i=0;i<n;i++)
    {
        cin>>a[i];
        sum1+=a[i];
        if(sum1>max1)
        max1=sum1;
    }
    int m;
    cin>>m;
    int b[m],sum2=0,max2=0;
    for(int i=0;i<m;i++)
    {
        cin>>b[i];
        sum2+=b[i];
        if(sum2>max2)
        max2=sum2;
    }
    cout<<max1+max2<<"\n";}
}
